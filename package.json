{
  "name": "bookfetch",
  "version": "1.0.0",
  "description": "Fastify backend API service for extracting article content from HTML",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.18.1 <21.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "postbuild": "pnpm generate-token-on-build",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "generate-token": "tsx scripts/generate-token.ts",
    "generate-production-token": "tsx scripts/generate-production-token.ts",
    "generate-token-on-build": "tsx scripts/generate-token-on-build.ts"
  },
  "keywords": [
    "fastify",
    "typescript",
    "api",
    "html-extraction"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/jwt": "^8.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "pino-pretty": "^10.2.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

